{
  "grammars": [
    {
      "name": "man",
      "camelcase": "Man",
      "scope": "source.man",
      "path": ".",
      "file-types": [
        "man"
      ]
    }
  ],
  "metadata": {
    "version": "1.0.0",
    "license": "MIT",
    "description": "Best-effort Tree-sitter parser for Unix man pages",
    "authors": [
      {
        "name": "Riley Bruins"
      }
    ],
    "links": {
      "repository": "git+ssh://git@github.com/ribru17/tree-sitter-man.git"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true
  }
}
