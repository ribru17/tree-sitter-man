{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "man",
  "rules": {
    "manual": {
      "type": "SEQ",
      "members": [
        {
          "type": "ALIAS",
          "content": {
            "type": "PATTERN",
            "value": "[^\\r\\n]+"
          },
          "named": true,
          "value": "title"
        },
        {
          "type": "PATTERN",
          "value": "\\r?\\n"
        },
        {
          "type": "SYMBOL",
          "name": "_body"
        }
      ]
    },
    "_body": {
      "type": "REPEAT1",
      "content": {
        "type": "CHOICE",
        "members": [
          {
            "type": "SYMBOL",
            "name": "section_heading"
          },
          {
            "type": "SYMBOL",
            "name": "footer"
          },
          {
            "type": "PATTERN",
            "value": "\\r?\\n"
          }
        ]
      }
    },
    "block": {
      "type": "PREC_RIGHT",
      "value": 0,
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "SYMBOL",
            "name": "_content"
          },
          {
            "type": "REPEAT",
            "content": {
              "type": "CHOICE",
              "members": [
                {
                  "type": "SYMBOL",
                  "name": "_content"
                },
                {
                  "type": "PATTERN",
                  "value": "\\r?\\n"
                }
              ]
            }
          }
        ]
      }
    },
    "_content": {
      "type": "SEQ",
      "members": [
        {
          "type": "PATTERN",
          "value": " {7} *"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "reference"
            },
            {
              "type": "TOKEN",
              "content": {
                "type": "PREC",
                "value": -1,
                "content": {
                  "type": "PATTERN",
                  "value": "\\S+"
                }
              }
            }
          ]
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "CHOICE",
            "members": [
              {
                "type": "TOKEN",
                "content": {
                  "type": "PREC",
                  "value": -1,
                  "content": {
                    "type": "PATTERN",
                    "value": "\\S+"
                  }
                }
              },
              {
                "type": "SYMBOL",
                "name": "reference"
              },
              {
                "type": "PATTERN",
                "value": " +"
              }
            ]
          }
        },
        {
          "type": "PATTERN",
          "value": "\\r?\\n"
        }
      ]
    },
    "section_heading": {
      "type": "SEQ",
      "members": [
        {
          "type": "ALIAS",
          "content": {
            "type": "PATTERN",
            "value": "\\S[^\\r\\n]*\\r?\\n"
          },
          "named": true,
          "value": "section_title"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "CHOICE",
            "members": [
              {
                "type": "SYMBOL",
                "name": "block"
              },
              {
                "type": "SYMBOL",
                "name": "subsection_heading"
              }
            ]
          }
        }
      ]
    },
    "subsection_heading": {
      "type": "PREC_RIGHT",
      "value": 0,
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "PATTERN",
            "value": " {3}"
          },
          {
            "type": "ALIAS",
            "content": {
              "type": "PATTERN",
              "value": "\\S[^\\r\\n]*\\r?\\n"
            },
            "named": true,
            "value": "subsection_title"
          },
          {
            "type": "REPEAT",
            "content": {
              "type": "CHOICE",
              "members": [
                {
                  "type": "SYMBOL",
                  "name": "block"
                },
                {
                  "type": "SYMBOL",
                  "name": "option_section"
                },
                {
                  "type": "PATTERN",
                  "value": "\\r?\\n"
                }
              ]
            }
          }
        ]
      }
    },
    "reference": {
      "type": "PATTERN",
      "value": "[a-zA-Z0-9_]+\\(\\d+\\)"
    },
    "option_section": {
      "type": "PREC_RIGHT",
      "value": 0,
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "ALIAS",
            "content": {
              "type": "PATTERN",
              "value": " {7}([-+]\\S+( +[A-Z]+| +<\\S+>)?, +)*[-+]\\S+( +[A-Z]+\\s*\\r?\\n| +<\\S+>\\s*\\r?\\n)?"
            },
            "named": true,
            "value": "option"
          },
          {
            "type": "CHOICE",
            "members": [
              {
                "type": "PATTERN",
                "value": "[^\\r\\n]+"
              },
              {
                "type": "BLANK"
              }
            ]
          },
          {
            "type": "PATTERN",
            "value": "\\r?\\n"
          },
          {
            "type": "CHOICE",
            "members": [
              {
                "type": "SYMBOL",
                "name": "block"
              },
              {
                "type": "BLANK"
              }
            ]
          }
        ]
      }
    }
  },
  "extras": [],
  "conflicts": [],
  "precedences": [],
  "externals": [
    {
      "type": "SYMBOL",
      "name": "footer"
    }
  ],
  "inline": [],
  "supertypes": []
}
